{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "packages",
  "projects": {
    "memmy": {
      "projectType": "application",
      "schematics": {},
      "root": "packages/web",
      "sourceRoot": "packages/web/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "packages/web/dist/memmy",
            "index": "packages/web/src/index.html",
            "main": "packages/web/src/main.ts",
            "polyfills": "packages/web/src/polyfills.ts",
            "tsConfig": "packages/web/tsconfig.app.json",
            "aot": false,
            "assets": [
              "packages/web/src/favicon.ico",
              "packages/web/src/assets"
            ],
            "styles": [
              "packages/web/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "packages/web/src/environments/environment.ts",
                  "with": "packages/web/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "memmy:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "memmy:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "memmy:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "packages/web/src/test.ts",
            "polyfills": "packages/web/src/polyfills.ts",
            "tsConfig": "packages/web/tsconfig.spec.json",
            "karmaConfig": "packages/web/karma.conf.js",
            "assets": [
              "packages/web/src/favicon.ico",
              "packages/web/src/assets"
            ],
            "styles": [
              "packages/web/src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "packages/web/tsconfig.app.json",
              "packages/web/tsconfig.spec.json",
              "packages/web/e2e/tsconfig.json"
            ],
            "exclude": [
              "packages/web/**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "memmy:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "memmy:serve:production"
            }
          }
        }
      }
    },
    "ui": {
      "projectType": "library",
      "root": "packages/ui",
      "sourceRoot": "packages/ui/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "packages/ui/tsconfig.lib.json",
            "project": "packages/ui/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "packages/ui/src/test.ts",
            "tsConfig": "packages/ui/tsconfig.spec.json",
            "karmaConfig": "packages/ui/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "packages/ui/tsconfig.lib.json",
              "packages/ui/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }},
  "defaultProject": "memmy"
}
