{
  "name": "memmy",
  "version": "0.1.0",
  "description": "memmy app",
  "repository": "https://github.com/jamesbs/memmy",
  "author": "James Salas <osdev@jamesbs.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "model:test": "jest --config=model/jest.config.js",
    "gateway:build": "tsc -p tsconfig.gateway.build.json",
    "gateway:start": "ts-node -P tsconfig.gateway.json -r tsconfig-paths/register gateway/src/main.ts",
    "gateway:start:debug": "nodemon --config gateway/nodemon-debug.json",
    "gateway:test": "jest --config=gateway/jest.config.js",
    "gateway:test:watch": "jest --config=gateway/jest.config.js --watch",
    "gateway:test:cov": "jest --config=gateway/jest.config.js --cov",
    "gateway:test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --config=gateway/jest.config.js --runInBand",
    "web:start": "ng serve --ssl --source-map",
    "web:serve-e2e": "ng serve --source-map",
    "web:test": "ng test",
    "e2e": "npm run gateway:start && npm run web:serve-e2e && cypress:open",
    "cypress:open": "cypress open",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@angular-builders/jest": "8.0.3",
