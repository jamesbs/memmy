    "@types/mocha": "^5.2.7",
    "@types/node": "^12.6.9",
    "@types/ramda": "^0.26.9",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^3.4.5",
    "codelyzer": "^5.0.0",
    "cypress": "^3.4.0",
    "jest": "^24.8.0",
    "jest-preset-typescript": "^1.2.0",
    "lerna": "^3.15.0",
    "ng-packagr": "^5.1.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "selfsigned": "^1.10.4",
